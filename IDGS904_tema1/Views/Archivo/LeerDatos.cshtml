@{ 
    ViewBag.Title = "LeerDatos";
}
<h2>Leer Datos</h2>

@ViewBag.Archivo

@foreach(string tem in ViewBag.Archivo)
            {
                <li>
                    <ul>
                        @foreach(string item in tem.Split(','))
                        {
                            <li>@item</li>
                        }

                    </ul>
                </li>
            }
